# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here

# Bot Configuration
LOG_LEVEL=info

# Quiz Configuration
QUIZ_TIMEOUT=30
MAX_QUESTIONS=10
POINTS_CORRECT=10
POINTS_QUICK_BONUS=5

# Cache Configuration
CACHE_MAX_AGE_DAYS=7
CACHE_MAX_SIZE_MB=100
IMAGE_MAX_WIDTH=800
IMAGE_MAX_HEIGHT=600
IMAGE_QUALITY=85

# Wikipedia API Configuration
WIKIPEDIA_USER_AGENT=ArabCelebrityQuizBot/1.0
WIKIPEDIA_RATE_LIMIT=0.1

# Google Custom Search API Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_google_custom_search_engine_id_here

# Multi-Source Image Configuration
MULTI_SOURCE_ENABLED=true
MAX_IMAGES_PER_CELEBRITY=10
IMAGE_SOURCE_PRIORITY=wikipedia,google,instagram,twitter,facebook

# Fuzzy Matching Configuration
FUZZY_THRESHOLD=80
FUZZY_SUGGESTION_THRESHOLD=60 